  }%
}
\def\bxjl@rx@parse@g#1#2#3:#4:{%
  \bxjl@cond\ifnum#2>#4 \fi{%
    \bxjl@rx@seq{#1}{#4}\bxjl@rx@parse@h#3:#4:%
  }{%else
    \bxjl@rx@seq{#1}{#2}\bxjl@rx@parse@h#3:#4:%
  }%
}
\def\bxjl@rx@parse@h#1#2:#3:#4:{%
  \expandafter\bxjl@rx@parse@a\bxjl@burst-`>#2:#3:#4#1:%
}

